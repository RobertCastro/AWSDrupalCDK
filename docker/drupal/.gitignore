# Ignore directories managed by Composer
/vendor/
/core/
/modules/contrib/
/themes/contrib/
/profiles/contrib/

# Ignore sensitive directories
/sites/*/files/
/sites/*/tmp/

# Ignore specific files that may contain sensitive information
/sites/*/settings.php
/sites/*/settings.local.php
/sites/*/services.local.php

# Ignore other files and directories that should not be tracked
/.htaccess
/.gitattributes
/*.sql
/*.sql.gz
/*.tar.gz

# Theme NPM token
/themes/custom/*/.npmrc

# Theme node_modules
/themes/custom/*/node_modules/

# Ignore files generated by common IDEs
/.idea/
/.vscode/

# Ignore .env files as they are personal
/.env
/.editorconfig
/.gitattributes
/.dccache
.DS_Store

# Ignore other files
/.csslintrc
/.eslintignore
/.eslintrc.json
/.ht.router.php
/autoload.php
/example.gitignore